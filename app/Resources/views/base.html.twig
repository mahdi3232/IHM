        <!--Style que nous allons utiliser-->
        <link rel="stylesheet" href="{{asset('bootstrap/css/colorbox.css')}}"  />
        <link rel="stylesheet" href="{{asset('bootstrap/css/gabarits.css')}}"  />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />




